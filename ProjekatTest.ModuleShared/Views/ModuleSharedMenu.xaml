<UserControl x:Class="ProjekatTest.ModuleShared.Views.ModuleSharedMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProjekatTest.ModuleShared.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">
    <DockPanel>
        <Label Content="LOGO" />
        <Button x:Name="ButtonMenuConfigurations" Content="Configurations" Command="{Binding ButtonMenuConfigurationsClick}"/>
        <Label Content="Scripts"/>
        <Label Content="Scheduler"/>
        <Label Content="Reports"/>
        <Label Content="Options ICONS" HorizontalAlignment="Right"/>
    </DockPanel>
</UserControl>
